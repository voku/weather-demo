{
    "name": "voku/no-framework",
    "description": "An example weather project with a modern PHP backend.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Lars Moelleken",
            "email": "lars@moelleken.org",
            "homepage": "https://moelleken.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "8.1.*||8.2.*",
        "ext-pdo": "*",
        "ext-curl": "*",
        "laminas/laminas-diactoros": "3.2.*",
        "laminas/laminas-httphandlerrunner": "2.9.*",
        "middlewares/fast-route": "v2.0.*",
        "middlewares/request-handler": "v2.0.*",
        "php-di/php-di": "7.0.*",
        "relay/relay": "2.1.*",
        "twig/twig": "3.7.*",
        "voku/weather": "0.7.*",
        "voku/httpful": "3.0.*"
    }
    ,
    "require-dev": {
        "phpstan/phpstan": "1.10.*",
        "phpunit/phpunit": "10.5.*",
        "php-cs-fixer/shim": "v3.27.*",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "WeatherApp\\": "src/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public/"
    }
}
